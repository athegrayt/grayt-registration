{
  "env": {
    "browser": false,
    "commonjs": false,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "prefer-destructuring": "warn",
    "object-shorthand": "off",
    "no-underscore-dangle": "off"
  }
}
