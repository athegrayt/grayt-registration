{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": false
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "node/no-unsupported-features/es-syntax": [
      "off",
      { "ignores": ["modules"] }
    ],
    "react/prop-types": "off"
  }
}
